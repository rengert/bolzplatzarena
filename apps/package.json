{
  "name": "bolzplatzarena",
  "version": "0.0.0",
  "scripts": {
    "lint:all": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng lint",
    "test:all": "ng test --watch=false --browsers=ChromeHeadless",
    "build:snake": "ng build snake --prod",
    "lint:snake": "ng lint snake",
    "build:ultradice": "ng build ultradice --prod",
    "lint:ultradice": "ng lint ultradice",
    "build:startup": "ng build startup --prod",
    "lint:startup": "ng lint startup",
    "test:startup": "ng test startup --watch=false --browsers=ChromeHeadless",
    "build:nonogramm": "ng build nonogramm --prod",
    "lint:nonogramm": "ng lint nonogramm",
    "test:nonogramm": "ng test nonogramm --watch=false --browsers=ChromeHeadless",
    "deploy:ascii-racer": "ng build ascii-racer --prod && firebase deploy --only hosting:ascii-racer",
    "deploy:nonogramm": "ng build nonogramm --prod && firebase deploy --only hosting:nonogramm",
    "deploy:startup": "ng build startup --prod && firebase deploy --only hosting:thestartup",
    "deploy:snake": "ng build snake --prod && firebase deploy --only hosting:rattlesnake",
    "deploy:snake3d": "ng build snake3d --prod && firebase deploy --only hosting:rattlesnake3d",
    "nonogramm": "ng serve nonogramm",
    "startup": "ng serve startup",
    "sync snake ios": "cd projects/snake/apps && npx cap sync ios  && npx cap open ios",
    "sync snake3d ios": "cd projects/snake3d/apps && npx cap sync ios",
    "sync nonogramm ios": "cd projects/nonogramm/apps && npx cap sync ios",
    "open nonogramm ios": "cd projects/nonogramm/apps && npx cap open ios"
  },
  "private": true,
